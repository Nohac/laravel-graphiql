{
  "name": "noh4ck/graphiql",
  "description": "Graphiql UI for laravel",
  "keywords": [
    "laravel",
    "graphql",
    "graphiql"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Jonas Rudlang",
      "email": "jonas.rudlang@gmail.com"
    }
  ],
  "require": {
    "php": ">=5.5.9",
    "laravel/framework": "5.1.*|5.2.*|5.3.*|5.4.*|5.5.*|5.6.*|5.7.*"
  },
  "autoload": {
    "psr-4": {
      "Graphiql\\": "src"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "laravel": {
        "providers": [
            "Graphiql\\GraphiqlServiceProvider"
        ]
    }
  }
}
